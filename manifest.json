{
  "manifest_version": 2,

  "name": "play",
  "version": "0.1",
  "description": "Control media playing in background tabs.",

  "browser_action": {
    "default_icon": "iconpeek.png"
  },

  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  
  "permissions": ["<all_urls>", "storage"]
}